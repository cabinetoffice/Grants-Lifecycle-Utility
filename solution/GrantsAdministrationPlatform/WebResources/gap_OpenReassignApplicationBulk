function openCustomPageDialog(
  primaryControl,
  firstSelectedItemId,
  selectedEntityTypeName,
  SelectedControlsSelectedItemIds
) {
  var itemIds = SelectedControlsSelectedItemIds.join(",");
  // Centered Dialog
  var pageInput = {
    pageType: "custom",
    name: "gap_reassignapplicationownerv2bulk_11c68",
    recordId: itemIds,
  };
  var navigationOptions = {
    target: 2,
    position: 1,
    height: {
      value: 260,
      unit: "px",
    },
    width: {
      value: 50,
      unit: "%",
    },
    title: "Reassign the Application",
  };
  Xrm.Navigation.navigateTo(pageInput, navigationOptions)
    .then(function () {
      // Refresh the main form when the dialog is closed
      primaryControl.data.refresh();
    })
    .catch(function (error) {
      console.log(error);
    });
}
